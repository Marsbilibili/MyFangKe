<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="popwin_near_anim_style">
        <item name="android:windowEnterAnimation">@anim/popupwindow_near_in_anim</item>
        <item name="android:windowExitAnimation">@anim/popupwindow_near_out_anim</item>
    </style>
    <style name="popwin_price_btn_anim_style">
        <item name="android:windowEnterAnimation">@anim/popupwindow_price_btn_in_anim</item>
        <item name="android:windowExitAnimation">@anim/popupwindow_price_btn_out_anim</item>
    </style>
    <style name="rentalhouse_popwin_near_anim_style">
        <item name="android:windowEnterAnimation">@anim/rentalhouse_popupwindow_near_in_anim</item>
        <item name="android:windowExitAnimation">@anim/rentalhouse_popupwindow_near_out_anim</item>
    </style>

</resources>
